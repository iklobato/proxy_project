{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "financialdata-cache-worker-staging",
	"main": "src/index.js",
	"compatibility_date": "2025-05-31",
	"observability": {
		"enabled": true
	},
	"placement": { 
		"mode": "smart" 
	},
	"vars": {
		"ENVIRONMENT": "staging",
		"VERSION": "staging-latest"
	}
	
	/**
	 * Staging Environment Configuration
	 * 
	 * Add your Account ID when ready to deploy:
	 * "account_id": "YOUR_ACCOUNT_ID_HERE",
	 * 
	 * For custom domain routing:
	 * "routes": [
	 *   {
	 *     "pattern": "cache-staging.financialdata.online/*",
	 *     "zone_id": "YOUR_ZONE_ID_HERE"
	 *   }
	 * ]
	 */
} 